<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cn.wiz.example.MainActivity">

    <ImageView
        android:id="@+id/launch_note"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_centerInParent="true"
        android:src="@drawable/icon_launcher_mi"/>
    <Button
        android:id="@+id/sdk_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/launch_note"
        android:layout_centerHorizontal="true"
        android:enabled="false"
        android:text="SDK未初始化,点击图标初始化SDK"/>

    <Button
        android:id="@+id/test_webview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/sdk_status"
        android:layout_centerHorizontal="true"
        android:text="测试WebView"/>

</RelativeLayout>
